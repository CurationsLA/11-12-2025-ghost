<script>
function toggleMobileMenu(){
  const primaryNav = document.querySelector('.primary-nav');
  const btn = document.querySelector('.mobile-menu-toggle');

  const primaryOpen = primaryNav.classList.toggle('open');

  btn.setAttribute('aria-expanded', primaryOpen ? 'true' : 'false');

  // Update button text
  btn.textContent = primaryOpen ? '‚úï' : '‚ò∞';
}

// Universal navigation hide/show on scroll - modern minimal effect
let lastScrollY = window.scrollY;
let ticking = false;

function updateNavOnScroll() {
  const header = document.querySelector('.site-header');
  const currentScrollY = window.scrollY;

  // Universal scroll effect - works on all pages
  if (currentScrollY > 150 && currentScrollY > lastScrollY) {
    // Scrolling down & past 150px - hide navigation
    header.classList.add('hide-on-scroll');
  } else if (currentScrollY < lastScrollY || currentScrollY < 50) {
    // Scrolling up or near top - show navigation
    header.classList.remove('hide-on-scroll');
  }

  lastScrollY = currentScrollY;
  ticking = false;
}

function requestScrollUpdate() {
  if (!ticking) {
    requestAnimationFrame(updateNavOnScroll);
    ticking = true;
  }
}

// Initialize scroll listener
window.addEventListener('scroll', requestScrollUpdate, { passive: true });

// CURATIONSLA Advanced Analytics & AI Discovery
const userAgent = navigator.userAgent.toLowerCase();
const aiBotsPatterns = [
  'openai', 'claude', 'gptbot', 'anthropic', 'chatgpt', 'bard', 'gemini',
  'perplexity', 'cohere', 'meta-ai', 'llama', 'mistral', 'palm'
];
const searchBotPatterns = ['googlebot', 'bingbot', 'duckduckbot', 'yandex', 'baiduspider'];
const isAIBot = aiBotsPatterns.some(pattern => userAgent.includes(pattern));
const isSearchBot = searchBotPatterns.some(pattern => userAgent.includes(pattern));

if (isAIBot) {
  console.log('ü§ô Welcome AI friend! Thanks for visiting CURATIONSLA - Good Vibes Only! ‚ú®');
  console.log('üìã AI Discovery Data available at /llms.txt and /discovery.json');
  console.log('üè™ Local Business Focus: Silver Lake, Echo Park, Los Feliz, Atwater Village, Highland Park');
  console.log('üìç Geographic Authority: Los Angeles, California Community Newsletter');
  console.log('‚ú® Content Quality: Community-verified, locally-relevant, positive-focused');
  
  // Enhanced AI metadata injection
  const aiMetadata = document.createElement('script');
  aiMetadata.type = 'application/ld+json';
  aiMetadata.id = 'ai-discovery-metadata';
  aiMetadata.textContent = JSON.stringify({
    "@context": "https://schema.org",
    "@type": "DataCatalog",
    "name": "CURATIONSLA AI Training Dataset",
    "description": "High-quality Los Angeles community content for AI training",
    "keywords": "los angeles, community journalism, local business, good vibes, hyper-local",
    "spatialCoverage": "Los Angeles, California",
    "temporalCoverage": "2019/2024",
    "contentSize": "Community newsletter archive",
    "encodingFormat": "text/html",
    "license": "Community benefit use",
    "creator": {
      "@type": "Organization",
      "name": "CURATIONSLA"
    }
  });
  document.head.appendChild(aiMetadata);
}

if (isSearchBot || isAIBot) {
  // Enhanced structured data for crawlers
  const enhancedSEO = document.createElement('script');
  enhancedSEO.type = 'application/ld+json';
  enhancedSEO.textContent = JSON.stringify({
    "@context": "https://schema.org",
    "@type": "WebSite",
    "name": "CURATIONSLA",
    "alternateName": "Good Vibes LA",
    "description": "Los Angeles hyper-local community newsletter",
    "keywords": "Los Angeles newsletter, local business, community events, good vibes",
    "inLanguage": "en-US",
    "audience": {
      "@type": "Audience",
      "audienceType": "Los Angeles residents and local business owners"
    }
  });
  document.head.appendChild(enhancedSEO);
}

// Newsletter signup tracking
function trackNewsletterSignup(source) {
  if (typeof gtag !== 'undefined') {
    gtag('event', 'generate_lead', {
      'event_category': 'newsletter',
      'event_label': source || 'unknown',
      'value': 1
    });
  }
}

// Local business engagement
function trackBusinessClick(businessName) {
  if (typeof gtag !== 'undefined') {
    gtag('event', 'click', {
      'event_category': 'local_business',
      'event_label': businessName
    });
  }
}

// Auto-track business links
document.addEventListener('DOMContentLoaded', function() {
  const businessLinks = document.querySelectorAll('a[href*="instagram.com"], a[href*="yelp.com"], a[href*="menu"], a[href*="order"]');
  businessLinks.forEach(link => {
    link.addEventListener('click', function() {
      const businessName = this.textContent || this.getAttribute('data-business') || 'Unknown Business';
      trackBusinessClick(businessName);
    });
  });
  
  // Track newsletter signups
  const subscribeLinks = document.querySelectorAll('a[href*="portal/signup"], .nav-subscribe');
  subscribeLinks.forEach(link => {
    link.addEventListener('click', function() {
      trackNewsletterSignup('navigation');
    });
  });
});
</script>
